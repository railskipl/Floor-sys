<h1>Listing product_backings</h1>

<table>
  <tr>
    <th>Name 1</th>
    <th>Name 2</th>
    <th>Company</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product_backings.each do |product_backing| %>
  <tr>
    <td><%= product_backing.name_1 %></td>
    <td><%= product_backing.name_2 %></td>
    <td><%= product_backing.company_id %></td>
    <td><%= link_to 'Show', product_backing %></td>
    <td><%= link_to 'Edit', edit_product_backing_path(product_backing) %></td>
    <td><%= link_to 'Destroy', product_backing, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

