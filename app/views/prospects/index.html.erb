<h1>Listing prospects</h1>

<table>
  <tr>
    <th>Telephone</th>
    <th>Online</th>
    <th>Walk in</th>
    <th>Product interest</th>
    <th>Quotation number sent</th>
    
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @prospects.each do |prospect| %>
  <tr>
    <td><%= prospect.telephone %></td>
    <td><%= prospect.online %></td>
    <td><%= prospect.walk_in %></td>
    <td><%= prospect.product_interest %></td>
    <td><%= prospect.quotation_number_sent %></td>
    <td><%= link_to 'Show', prospect %></td>
    <td><%= link_to 'Edit', edit_prospect_path(prospect) %></td>
    <td><%= link_to 'Destroy', {:controller=>'prospects', :action=>'destroy', :id=>"#{prospect.id}"}, :confirm => 'Are you sure?' %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Prospect', new_prospect_path %>
