<h1>Listing quotations</h1>

<table>
  <tr>
    <th>Product</th>
    <th>Uom</th>
    <th>Supplier</th>
    <th>Company</th>
    <th>Quotation number</th>
    <th>Date generated</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @quotations.each do |quotation| %>
  <tr>
    <td><%= quotation.product_id %></td>
    <td><%= quotation.uom_id %></td>
    <td><%= quotation.supplier_id %></td>
    <td><%= quotation.company_id %></td>
    <td><%= quotation.quotation_number %></td>
    <td><%= quotation.date_generated %></td>
    <td><%= link_to 'Show', quotation %></td>
    <td><%= link_to 'Edit', edit_quotation_path(quotation) %></td>
    <td><%= link_to 'Destroy', {:controller=>'quotations', :action=>'destroy', :id=>"#{quotation.id}"}, :confirm => 'Are you sure?' %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Quotation', new_quotation_path %>
